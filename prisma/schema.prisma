// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Evaluation {
  id        String   @id @default(uuid())
  user_id   String
  target_id String
  answers   Json
  created_at DateTime @default(now())

  @@index([user_id])
  @@index([target_id])
}
